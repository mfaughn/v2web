<?xml version="1.0" encoding="utf-8"?>
<DataTypeDefinition>
  <url value="http://hl7.org/v2plus/DataTypeDefinition/authorization-information"/>
  <name value="authorization information"/>
  <withdrawn value="false"/>
  <abbreviation value="AUI"/>
  <description>
    <status value="additional"/>
    <div>
      <p>Definition: This data type specifies the identifier or code for an insurance authorization instance and its associated detail.</p>
      <p><strong>Note:</strong> Replaces the CM data type used in sections 6.5.6.14 IN1-14, as of v2.5.</p>
    </div>
  </description>
  <component>
    <name value="Authorization Number"/>
    <sequenceNumber value="1"/>
    <definition>
      <status value="additional"/>
      <div>
        <p>Definition: Identifier assigned to the authorization.</p>
      </div>
    </definition>
    <confLength value="30"/>
    <mayTruncate value="false"/>
    <dataType value="https://www.hl7.org/v2plus/DataTypeDefinition/string-data"/>
  </component>
  <component>
    <name value="Date"/>
    <sequenceNumber value="2"/>
    <definition>
      <status value="additional"/>
      <div>
        <p>Definition: Date of authorization.</p>
      </div>
    </definition>
    <mayTruncate value="not applicable"/>
    <dataType value="https://www.hl7.org/v2plus/DataTypeDefinition/date"/>
  </component>
  <component>
    <name value="Source"/>
    <sequenceNumber value="3"/>
    <definition>
      <status value="additional"/>
      <div>
        <p>Definition: Source of authorization.</p>
      </div>
    </definition>
    <confLength value="199"/>
    <mayTruncate value="true"/>
    <dataType value="https://www.hl7.org/v2plus/DataTypeDefinition/string-data"/>
  </component>
</DataTypeDefinition>
