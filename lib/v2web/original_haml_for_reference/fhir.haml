  %script{:src => "./Resource - FHIR v4.1.0_files/jquery.js"} :cdata
  %script{:src => "./Resource - FHIR v4.1.0_files/jquery-ui.min.js"} :cdata
  :javascript
    try {
       var currentTabIndex = sessionStorage.getItem('fhir-resource-tab-index');
     } catch(exception){ 
     }
     if (!currentTabIndex)
       currentTabIndex = '0';
    
    $( '#tabs' ).tabs({ active: currentTabIndex, activate: function( event, ui ) { store(ui.newTab.index()); } });
    $( '#tabs-Meta' ).tabs({ active: currentTabIndex, activate: function( event, ui ) { store(ui.newTab.index()); } });
         
         
    function store(currentTab) {
      document.activeElement.blur();
      try {
        sessionStorage.setItem('fhir-resource-tab-index', currentTab);
      } catch(exception){ 
      }
      $( '#tabs' ).tabs('option', 'active', currentTab);
      $( '#tabs-Meta' ).tabs('option', 'active', currentTab);
         
    }
